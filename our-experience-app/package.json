{
  "name": "our-experience-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf ./build",
    "build": "npm run clean; tsc -p .; npm run copyfiles",
    "copyfiles": "copyfiles -e './node_modules/**/*' ./**/*.html ./**/*.css ./**/*.pug ./build",
    "start": "node ./bin/www"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "pug": "2.0.0-beta11"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/http-errors": "^1.8.1",
    "@types/node": "^16.11.9",
    "@types/pug": "^2.0.5",
    "copyfiles": "^2.4.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  }
}
